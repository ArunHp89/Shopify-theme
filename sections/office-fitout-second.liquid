<div class="office-fit-sec office-fit-sec-second blue-bg">
            <div class="office-fit-sec-wrapper">
                <h2 class="title h2">{{ section.settings.heading }}</h2>
                <div class="row">
                    {% for block in section.blocks %}
                      <div class="office-fit-col office-fit-col-6">
                        <div class="col-content">
                            {{ block.settings.text }}
                        </div>
                    </div>
                    {% endfor %}
                </div>
            </div>
        </div>

{% schema %}
  {
    "name": "Office Fitouts Second",
    "blocks": [
      {
        "type": "text",
        "name": "Enhanced Work Text",   
        "settings":[
              {
                "type": "richtext",
                "id": "text",
                "label": "Text",
                "default": "<p>Text</p>"
              }
        ]
      }   
    ],
    "settings": [
        {
          "type": "text",
          "id": "heading",
          "label": "Heading"
        } 
    ]
  }
{% endschema %}

{% stylesheet %}
{% endstylesheet %}

