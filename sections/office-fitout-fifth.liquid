<div class="office-fit-sec office-fit-sec-fifth" style= "background: url({{ section.settings.image | img_url: 'master' }}) 
          " >
    <div class="office-fit-sec-wrapper">
        <h2 class="title h2 text-center">{{ section.settings.heading }}</h2>
        &nbsp; &nbsp; &nbsp; &nbsp;
        <div class="row">
            <div class="content">
                <p>{{ section.settings.description }}</p>
            </div>
        </div>
    </div>
</div>

{% schema %}
  {
    "name": "Office Fitout Fifth",
    "settings": [
        {
          "type": "image_picker",
          "id": "image",
          "label": "Image"
        },
        {
          "type": "text",
          "id": "heading",
          "label": "Heading"
        },
        {
          "type": "text",
          "id": "description",
          "label": "Description"
        }      
    ]
  }
{% endschema %}

